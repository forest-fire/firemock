    "coveralls": "^2.0.0",
    "js-yaml": "^3.0.0",
    "mocha": "^3.4.0",
    "nyc": "^11.0.0",
    "prettier": "^1.5.2",
    "rimraf": "^2.0.0",
    "shelljs": "^0.7.8",
    "test-console": "^1.0.0",
    "ts-node": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "typescript": "^2.6.2"
  },
  "engines": {
    "node": ">=6.0.0"
