{
  "name": "firemock",
  "version": "0.17.0",
  "description": "firemock",
  "license": "MIT",
  "repository": "git@github.com:forest-fire/firemock.git",
  "author": "Ken Snyder<ken@ken.net>",
  "keywords": [
    "typescript",
    "firebase",
    "mocking",
    "testing"
  ],
  "files": [
    "lib"
  ],
  "main": "dist/index.cjs.js",
  "browser": "dist/index.min.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "tslint src/**/*.ts && tsc -P tsconfig.esnext.json && rollup -c rollup.cjs.js && rollup -c rollup.umd.js",
    "test": "ts-node ./scripts/test.ts",
    "coverage": "nyc --reporter=text --reporter=html mocha --compilers ts:ts-node/register",
    "watch": "ts-node ./scripts/watch.ts",
    "watch:test": "ts-node ./scripts/watch.ts test",
    "watch:all": "ts-node ./scripts/watch.ts all",
    "deploy": "ts-node ./scripts/deploy.ts "
  },
  "dependencies": {
    "common-types": "^1.7.36",
    "firebase-api-surface": "^0.8.0",
    "firebase-key": "^2.0.1",
    "lodash": "^4.17.10",
    "moment": "^2.20.1",
    "typed-conversions": "^0.8.3",
    "chance": "^1.0.16",
    "faker": "^4.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chance": "^0.7.33",
    "@types/commander": "^2.12.2",
    "@types/faker": "^4.1.0",
    "@types/js-yaml": "^3.10.0",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^2.2.41",
    "@types/node": "^6.0.0",
    "@types/rimraf": "^0.0.28",
    "@types/shelljs": "^0.7.1",
    "@types/stack-trace": "^0.0.28",
    "async-shelljs": "^0.1.2",
    "chai": "^4.1.2",
    "chalk": "^2.3.2",
    "commander": "^2.15.0",
    "coveralls": "^2.0.0",
    "faker": "^4.1.0",
    "firebase-admin": "^5.12.0",
    "js-yaml": "^3.11.0",
    "mocha": "^5.2.0",
    "nyc": "^11.0.0",
    "prettier": "^1.13.4",
    "rimraf": "^2.6.2",
    "test-console": "^1.1.0",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^3.0.0-dev.20180522"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
