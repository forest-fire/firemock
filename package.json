    "moment": "^2.20.0",
    "lodash": "^4.17.4",
    "typed-conversions": "^0.7.3"
  },
  "peerDependencies": {
    "chance": "^1.0.0",
    "faker": "^4.1.0"
  },
  "devDependencies": {
    "@types/chai": "^3.0.0",
    "@types/chalk": "^0.4.31",
    "@types/chance": "^0.7.33",
    "@types/faker": "^4.1.0",
    "@types/js-yaml": "^3.9.0",
