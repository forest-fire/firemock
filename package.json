    "chalk": "^3.0.0",
    "do-devops": "^0.3.2",
    "mocha": "^6.2.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "test-console": "^1.1.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.3",
