    "faker": "^4.1.0",
    "fast-copy": "^2.0.4",
    "fast-equals": "^2.0.0",
    "firebase-key": "^2.0.2",
    "lodash.first": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.last": "^3.0.0",
    "lodash.set": "^4.3.2",
    "serialized-query": "^0.10.0",
    "typed-conversions": "^0.10.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/deepmerge": "^2.2.0",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash.first": "^3.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.last": "^3.0.6",
    "@types/lodash.set": "^4.3.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^8.0.0",
