  "keywords": ["serverless", "typescript", "firebase", "mocking", "testing"],
  "files": ["lib"],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
